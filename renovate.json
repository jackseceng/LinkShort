{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "automergeType": "pr",
  "prCreation": "immediate",
  "extends": [
    "config:base",
    ":automergeRequireAllStatusChecks",
    ":ignoreUnstable",
    ":rebaseStalePrs"
  ],
  "packageRules": [
    {
      "assignees": [
        "jackseceng"
      ],
      "assigneesSampleSize": 2,
      "bumpVersion": "patch",
      "commitMessagePrefix": "chore(deps)",
      "labels": [
        "maintenance", 
        "renovate"
      ],
      "matchConfidence": [
        "high", "very high"
      ]
    }
  ]
}
